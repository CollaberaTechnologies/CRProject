    }

    label.cust-title {
        font-weight: 400;
        background: 0 0;
        float: left;
        font-size: 14px;
        padding: 5px 0 5px 5px;
        width: 100%;
        margin-bottom: 0;
    }

        label.cust-title label {
            margin-right: 15px;
            font-weight: 400;
        }

