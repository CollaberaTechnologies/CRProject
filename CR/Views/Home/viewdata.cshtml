        float: none !important;
        margin-right: auto !important;
        margin-left: auto !important;
    }

    .quecat {
        padding: 0;
        margin: 0;
        text-transform: uppercase;
        color: #fff;
        font-size: 22px;
        width: 80px;
        background-color: #585858;
