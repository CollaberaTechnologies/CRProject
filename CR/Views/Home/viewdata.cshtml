            font-size: 1em;
            padding: 15px 22px 5px;
        }

        .wrong-pass img {
            float: left;
            margin: 30px 15px 30px 30px;
        }

    span.vertical-line {
        width: 1px;
        background-color: silver;
        height: 100%;
        border: 2px ridge silver;
        border-radius: 2px;
    }

    .box {
        width: 45%;
        height: auto;
        background: #FFF;
        margin: 40px auto;
        padding: 40px 0;
    }



    .effect8 {
        position: relative;
        box-shadow: 0 1px 4px rgba(0, 0, 0, .3), 0 0 40px rgba(0, 0, 0, .1) inset;
    }

        .effect8:after,
        .effect8:before {
            content: "";
            position: absolute;
            z-index: -1;
            box-shadow: 0 0 20px rgba(0, 0, 0, .8);
            top: 10px;
            bottom: 10px;
            left: 0;
            right: 0;
            border-radius: 100px/10px;
        }

        .effect8:after {
            right: 10px;
            left: auto;
            transform: skew(8deg) rotate(3deg);
        }

    .box h3 {
        position: relative;
        top: 80px;
    }

    .box h1 {
        margin: 20px 0;
    }

    h2 {
        color: red;
    }

    .sweet-alert .sa-icon {
        width: 80px;
        height: 80px;
        border: 4px solid gray;
        border-radius: 50%;
        margin: 20px auto 30px;
        position: relative;
        box-sizing: content-box;
    }

    .country-img {
        display: block;
        position: relative;
        margin: 10px 20px;
        cursor: pointer;
        z-index: 11;
        border: 3px solid #fff;
        box-shadow: 0 0 14px 4px rgba(185, 185, 185, 0.57);
        ;
    }

        .country-img img {
            display: block;
            margin: 0 auto;
            width: 100%;
        }

        .country-img p.caption {
            position: relative;
            display: block;
            width: 100%;
            text-align: center;
            background: #009bde;
            /* left: 0;
                bottom: -100px; */
            padding: 10px 0;
            transition: .3s all ease-in-out;
        }

        .country-img p.caption {
            color: #fff;
            margin-bottom: 0;
            font-size: 16px;
        }

    .form-control.k-widget {
        padding: 0;
        width: 100%;
    }

    /* .country-img:hover .caption {
                bottom: 0
            } */
    .bootstrap-tagsinput {
        max-width: unset;
        width: 100%;
        font-family: 'Roboto', sans-serif;
    }

    .radio-grp h4 {
        margin-left: 20px;
        font-family: 'Roboto',sans-serif;
        font-size: 15px;
    }

        .radio-grp h4 input[type="radio"] {
            margin: 2px 5px 2px;
        }

    .datepicker > div {
        display: unset;
    }
</style>

